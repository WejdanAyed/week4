#Week4 , Day22
thissdict2 = {
    "brand" : "Ford" ,
    "mode1" : "Mustang" , 
    "year" : 1964
    }
v = thissdict2 [ "mode1" ]
print(v)

v = thissdict2.get ("year")
print(v)

thissdict2["brand"] = "B"
print(thissdict2)

for y in thissdict2:
    print(thissdict2[y])

for x , y in thissdict2.items ():
    print(x , y)


#Week4 , Day23
thissdict3 = {
    "brand1" : "Ford" ,
    "mode11" : "Mustang" , 
    "year1" : 1964
    }
if "mode11" in thissdict3:
    print("Yes")

print(len(thissdict3))

thissdict3["color"] = "red"
print(thissdict3)

thissdict3.pop("brand1")
print(thissdict3)

thissdict3.popitem()
print(thissdict3)

thissdict3.clear()
print(thissdict3)


